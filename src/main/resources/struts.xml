<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
    "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>

	<constant name="struts.devMode" value="true" />

	<package name="web" extends="struts-default">
		<action name="EncodingLogin">
			<result>/register.jsp</result>
		</action>
		<action name="test" method="register" class="controller.LoginController">
			<result name="success" type="redirect">updateUserList</result>
		</action>
		<action name="updateUserList" method="updateUserList"
			class="controller.LoginController">
			<result name="success">/register.jsp</result>
		</action>
	</package>

</struts>